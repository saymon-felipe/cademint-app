tion\").val();\r\n            $(\"#write-publication\").val(\"\");\r\n            $(\"#write-publication\").css(\"height\", \"38.78px\");\r\n            $(\"#write-publication\").attr(\"rows\", 1);\r\n            button.removeClass();\r\n            button.addClass(\"publish-disabled\");\r\n            button.attr(\"disabled\", \"disabled\");\r\n            $(\"#publish\").hide();\r\n        },\r\n        addImageToPublication() {\r\n            console.log(\"adicionar imagem à publicação\");\r\n        },\r\n        addVideoToPublication() {\r\n            console.log(\"adicionar video à publicação\");\r\n        },\r\n        showPublishButton() {\r\n            var textArea = $(\"#write-publication\");\r\n            var button = $(\"#publish\");\r\n            if(textArea.val() == \"\") {\r\n                button.attr(\"disabled\", \"disabled\");\r\n                button.removeClass();\r\n                button.addClass(\"publish-disabled\");\r\n            } else {\r\n                button.removeAttr(\"disabled\");\r\n                button.removeClass();\r\n                button.addClass(\"publish-enabled\");\r\n            }\r\n        }\r\n    }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n    .new-publication {\r\n        width: 100%;\r\n        background: var(--white);\r\n        border-radius: 10px;\r\n        box-shadow: 0 0 10px rgba(0,0,0,0.2);\r\n        padding: .4rem 1rem;\r\n        display: flex;\r\n        flex-direction: column;\r\n        align-items: center;\r\n        margin-top: 1.5rem;\r\n    }\r\n\r\n    .new-publication i {\r\n        font-size: 1.5rem;\r\n    }\r\n\r\n    .new-publication-header {\r\n        width: 100%;\r\n        display: flex;\r\n        justify-content: space-around;\r\n    }\r\n\r\n    .new-publication-header textarea {\r\n        resize: none;\r\n        border: 1px solid var(--gray-medium);\r\n        background: var(--gray-high);\r\n        border-radius: 10px;\r\n        padding: .4rem;\r\n        width: 100%;\r\n    }\r\n\r\n    .new-publication-text {\r\n        display: flex;\r\n        align-items: flex-end;\r\n        margin-left: 1rem;\r\n        width: 100%;\r\n        margin-top: auto;\r\n        margin-bottom: auto;\r\n    }\r\n\r\n    #publish {\r\n        color: var(--white);\r\n        padding: .2rem .7rem;\r\n        border: none;\r\n        border-radius: 10px;\r\n        margin-left: -95px;\r\n        margin-bottom: 10px;\r\n    }\r\n\r\n    .publish-enabled {\r\n        background: var(--blue);\r\n        cursor: pointer\r\n    }\r\n\r\n    .publish-disabled {\r\n        background: var(--blue-high);\r\n        cursor: not-allowed;\r\n    }\r\n\r\n    #publish:active {\r\n        background: var(--blue-low);\r\n    }\r\n\r\n    #write-publication {\r\n        cursor: text;\r\n        transition: 0.3s height;\r\n    }\r\n\r\n    .media-icons {\r\n        display: flex;\r\n        align-items: center;\r\n        width: 100%;\r\n        height: 2rem;\r\n    }\r\n\r\n    .add-image, .add-video {\r\n        width: 50%;\r\n        height: 100%;\r\n        display: flex;\r\n        align-items: center;\r\n        justify-content: center;\r\n        cursor: pointer;\r\n    }\r\n\r\n    .add-image:hover, .add-video:hover {\r\n        background: var(--gray-high);\r\n    }\r\n\r\n    .add-image i {\r\n        color: var(--yellow)\r\n    }\r\n    \r\n    .add-video i {\r\n        color: var(--blue)\r\n    }\r\n\r\n    @media (max-width: 420px) {\r\n        .add-image i, .add-video i {\r\n            font-size: 1.3rem;\r\n        }\r\n        .new-publication-header textarea {\r\n            padding: .2rem .5rem;\r\n        }\r\n        .new-publication {\r\n            margin-top: 1rem;\r\n        }\r\n    }\r\n</style>"],"sourceRoot":"src/components"}]}